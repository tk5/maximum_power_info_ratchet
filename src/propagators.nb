Cell[TextData[{
 "Now we derive the propagators associated with the trap reacting to noisy \
measurements y = x + \[Xi], where \[Xi] is a Gaussian random variable with \
zero mean and std. deviation \[Sigma], as well as for rule ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Lambda]", 
     RowBox[{"n", "+", "1"}]], "=", 
    RowBox[{
     SubscriptBox["\[Lambda]", "n"], "+", 
     RowBox[{"\[Alpha]", "*", 
      RowBox[{"\[CapitalTheta]", "(", 
       RowBox[{
        SubscriptBox["x", 
         RowBox[{"n", "+", "1"}]], "-", 
        SubscriptBox["\[Lambda]", "n"]}], ")"}], 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["x", 
         RowBox[{"n", "+", "1"}]], "-", 
        SubscriptBox["\[Lambda]", "n"]}], ")"}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "2717b6d6-df58-4d15-a4a9-d252d8b585aa"],
 ": "
}], "Text",
