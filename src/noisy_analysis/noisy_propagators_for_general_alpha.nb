 CellLabel->"Out[37]=",ExpressionUUID->"4f0b3b8e-0531-4c5d-a91a-8a600d6b2c4f"],

Cell[BoxData[
 TemplateBox[{
   FractionBox[
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{"-", 
       FractionBox[
        SuperscriptBox[
         RowBox[{"(", 
           RowBox[{"lkp", "+", 
             RowBox[{"lk", " ", 
               RowBox[{"(", 
