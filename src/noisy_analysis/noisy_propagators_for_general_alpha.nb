 CellLabel->"In[36]:=",ExpressionUUID->"7ef55fd6-abba-4d1e-ab51-7148a8c229b6"],

Cell["\<\
We convolve the trap position propagator with the noise kernel to get a \
propagator purely in terms of the true particle position and the trap \
position. Mathematica doesn\[CloseCurlyQuote]t easily do this integral so we \
manually do this term-by-term. The two terms are given by:\
\>", "Text",
 CellChangeTimes->{{3.824757924968135*^9, 3.824757928613255*^9}, {
  3.8291749134853897`*^9, 3.8291749362545223`*^9}, {3.8291750228279057`*^9, 
  3.829175038290002*^9}},
 Background->RGBColor[
  0.87, 0.94, 1],ExpressionUUID->"d2d10c02-e507-4ede-94de-fe14fe388ec2"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"Integrate", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"DiracDelta", "[", 
        RowBox[{"lkp", "-", "lk"}], "]"}], 
       RowBox[{"HeavisideTheta", "[", 
        RowBox[{"-", 
         RowBox[{"(", 
          RowBox[{"ykp", "-", "lk"}], ")"}]}], "]"}]}], " ", ")"}], " ", 
     RowBox[{"pn", "[", 
      RowBox[{"ykp", ",", "xkp", ",", " ", "sn"}], "]"}]}], ",", 
    RowBox[{"{", 
     RowBox[{"ykp", ",", 
      RowBox[{"-", "\[Infinity]"}], ",", "\[Infinity]"}], "}"}]}], "]"}], "//",
   "Simplify"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Integrate", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"DiracDelta", "[", 
        RowBox[{"lkp", "-", 
         RowBox[{"(", 
          RowBox[{"lk", "+", 
           RowBox[{"\[Alpha]", 
            RowBox[{"(", 
             RowBox[{"ykp", "-", "lk"}], ")"}]}]}], ")"}]}], "]"}], 
       RowBox[{"HeavisideTheta", "[", 
        RowBox[{"ykp", "-", "lk"}], "]"}]}], ")"}], " ", 
     RowBox[{"pn", "[", 
      RowBox[{"ykp", ",", "xkp", ",", " ", "sn"}], "]"}]}], ",", 
    RowBox[{"{", 
     RowBox[{"ykp", ",", 
      RowBox[{"-", "\[Infinity]"}], ",", "\[Infinity]"}], "}"}]}], "]"}], "//",
   "Simplify"}]}], "Input",
 CellChangeTimes->{{3.82475796040399*^9, 3.824757989125218*^9}, {
   3.824758533253065*^9, 3.8247585348814774`*^9}, 3.824758574270883*^9, {
   3.82475865241849*^9, 3.824758699541768*^9}},
 CellLabel->"In[37]:=",ExpressionUUID->"6dc77386-42a4-4364-8f38-1fe6930246ca"],

Cell[BoxData[
 RowBox[{
  FractionBox["1", "2"], " ", 
  RowBox[{"DiracDelta", "[", 
   RowBox[{
    RowBox[{"-", "lk"}], "+", "lkp"}], "]"}], " ", 
  RowBox[{"(", 
   RowBox[{"1", "+", 
    RowBox[{"Erf", "[", 
     FractionBox[
      RowBox[{"lk", "-", "xkp"}], 
      RowBox[{
       SqrtBox["2"], " ", "sn"}]], "]"}]}], ")"}]}]], "Output",
 CellChangeTimes->{
  3.824757998589451*^9, 3.824758444546678*^9, {3.824758547047822*^9, 
